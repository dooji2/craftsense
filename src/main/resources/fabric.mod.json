{
	"schemaVersion": 1,
	"id": "craftsense",
	"version": "${version}",
	"name": "CraftSense",
	"description": "A Minecraft mod that predicts crafting recipes in real-time, offering previews based on items in the crafting grid and adapting to your crafting habits over time.",
	"authors": [
		"Dooji"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/craftsense/",
		"sources": "https://github.com/dooji2/craftsense"
	},
	"license": "GPL-v3",
	"icon": "assets/craftsense/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.dooji.craftsense.CraftSense"
		]
	},
	"mixins": [
		"craftsense.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.7",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}